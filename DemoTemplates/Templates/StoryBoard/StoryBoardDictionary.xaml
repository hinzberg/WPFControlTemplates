<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DemoTemplates.Templates.StoryBoard">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Templates/StoryBoard/ConstDictionary.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Storyboard x:Key="StoryboardAnimationNameMissing" BeginTime="0:0:0" 
                                    RepeatBehavior="Forever" 
                                    Storyboard.TargetProperty="(Foreground).(SolidColorBrush.Color)"
                                    >
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Width">
            <LinearDoubleKeyFrame Value="55" KeyTime="0:0:0"/>
            <LinearDoubleKeyFrame Value="70" KeyTime="0:0:1"/>
            <LinearDoubleKeyFrame Value="85" KeyTime="0:0:2"/>
            <LinearDoubleKeyFrame Value="70" KeyTime="0:0:3"/>
            <LinearDoubleKeyFrame Value="55" KeyTime="0:0:4"/>
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>
    
</ResourceDictionary>