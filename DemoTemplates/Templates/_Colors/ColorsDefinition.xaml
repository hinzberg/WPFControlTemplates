<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--<SolidColorBrush x:Key="PrimaryBrush" Color="#525E91"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="#DEE5F6"/>-->

    <SolidColorBrush x:Key="PrimaryBrush" Color="Red"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="Green"/>
    
    <SolidColorBrush x:Key="EnabledBrush" Color="Silver"/>
    <SolidColorBrush x:Key="SecondaryEnabledBrush" Color="Gray"/>

    <!-- Button-->
    <SolidColorBrush x:Key="ButtonTextBrush" Color="White" />
    <SolidColorBrush x:Key="ButtonMouseoverTextBrush" Color="White" />
    <SolidColorBrush x:Key="ButtonClickTextBrush" Color="White" />

    <!-- CheckBox-->
    <SolidColorBrush x:Key="CheckBoxBackgroundBrush" Color="White"/>

    <!-- RadioButton-->
    <SolidColorBrush x:Key="RadioButtonBackground" Color="White"/>

    <!-- CextBox / PasswordBox-->
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="White"/>

    <!--  ToolTip-->
    <SolidColorBrush x:Key="ToolTipTextColor" Color="White"/>

    <!-- ComboBox-->
    <Color x:Key="ComboBoxArrowColor">white</Color>
    <SolidColorBrush x:Key="ComboBoxDropDownBorderBackground" Color="White"/>
    <Color x:Key="ComboBoxToggleButtonInnerItemBackground">white</Color>
    <SolidColorBrush x:Key="ComboBoxPartEditableTextBoxColor">transparent</SolidColorBrush>

</ResourceDictionary>